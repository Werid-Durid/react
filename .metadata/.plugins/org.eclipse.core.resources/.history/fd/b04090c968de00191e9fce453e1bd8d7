package 常用类;

public class 测试StringBulider {
	public static void main(String[] args) {
		StringBuilder sb=new StringBuilder();
		
		for(int i=0;i<26;i++) {
			sb.append(   (char)('a'+i) );
			
			
		}
		System.out.println(sb);
		
		sb.reverse();
		System.out.println(sb);
		
		sb.setCharAt(3, '是');
		System.out.println(sb);
		
	}

}
